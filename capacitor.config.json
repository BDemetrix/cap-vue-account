{
  "appId": "com.example.app",
  "appName": "cap-vue-account",
  "webDir": "dist",
  "bundledWebRuntime": true,
  "plugins": {
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["password"]
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  }
}
